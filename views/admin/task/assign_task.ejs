                                                <th>Assign name</th>
                                                <th>Description</th>
                                                <th>Priority</th>
                                                <th>Status</th>
                                                <th>Deadline</th>
                                            </tr>
                                        </thead>
                                    </table>
                                </div> <!-- end preview-->
                            </div> <!-- end tab-content-->
