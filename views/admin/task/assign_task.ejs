                    <div class="card">
                        <div class="card-body">

                            <div class="Task" style="display: flex; justify-content: space-between;">
                                <h4 class="Multi-iten" style="margin-bottom: .5rem; text-transform: uppercase; letter-spacing: .02em; font-size: .9rem;">Task Assignment</h4>
                                <button type="button" id="assignTaskButton" class="btn btn-outline-primary rounded-pill">Assign Task</button>
                            </div>
                            <div class="toggleform" id="popupOverlay" style="display: none;">
                                <form action="/admin/task" class="needs-validation" novalidate method="post">
                                    <div class="mb-3">
                                        <label for="simpleinput" class="form-label">Task Title</label>
                                        <input type="text" name="taskname" id="simpleinput" class="form-control">
                                    </div>
                                    <div class="mb-3">
                                        <label for="example-select-assign" class="form-label">Assign to</label>
                                        <select class="form-select" name="assignname" id="example-select-assign">
                                            <option>Select Person</option>
                                            <option value="Anand jethava">Anand Jethava</option>
                                            <option value="Sanjiv Singh">Sanjiv Singh</option>
                                            <option value="aditya rajput">Aditya Rajput</option>
                                            <option value="Piyush vaghela">Piyush Vaghela</option>
                                            <option value="Ankur Maurya">Ankur Maurya</option>
                                            <option value="Meet Gondaliya">Meet Gondaliya</option>
                                        </select>
                                    </div>
                                    <div class="mb-3">
                                        <label for="example-textarea" class="form-label">Description Task</label>
                                        <textarea class="form-control" name="description" id="example-textarea" rows="3"></textarea>
                                    </div>
                                    <div class="mb-3">
                                        <label for="example-select-priority" class="form-label">Priority</label>
                                        <select class="form-select" name="priority" id="example-select-priority">
                                            <option>High</option>
                                            <option>Medium</option>
                                            <option>Normal</option>
                                            <option>Low</option>
                                        </select>
                                    </div>
                                    <div class="mb-3">
                                        <label for="example-date" class="form-label">Deadline</label>
                                        <input class="form-control" id="example-date" type="date" name="deadline">
                                    </div>
                                    <input class="btn btn-primary" type="submit" value="Submit">
                                </form>
                            </div>
                            <div class="tab-content" style="margin-top: 20px;">
                                <div class="tab-pane show active" id="multi-item-preview">
                                    <table id="selection-datatable"
                                        class="table table-striped dt-responsive nowrap w-100">
                                        <thead>
